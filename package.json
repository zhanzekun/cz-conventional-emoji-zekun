{
  "name": "cz-conventional-emoji-zekun",
  "version": "1.0.2",
  "description": "A commitizen adapter following the conventional-changelog format with emoji.",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "mocha *.test.js",
    "format": "prettier --write *.js",
    "semantic-release": "semantic-release",
    "publish:npm": "npm publish --registry https://registry.npmjs.org/"
  },
  "homepage": "https://github.com/zhanzekun/cz-conventional-emoji-zekun",
  "repository": {
    "type": "git",
    "url": "https://github.com/zhanzekun/cz-conventional-emoji-zekun.git"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 10"
  },
  "author": "zekunzhan<zhanzekun@foxmail.com>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.4.1",
    "commitizen": "^4.0.3",
    "conventional-commit-types": "^3.0.0",
    "lodash.map": "^4.5.1",
    "longest": "^2.0.1",
    "right-pad": "^1.0.1",
    "word-wrap": "^1.0.3"
  },
  "devDependencies": {
    "cosmiconfig": "^5.2.1",
    "prettier": "^1.15.3",
    "semantic-release": "^19.0.3",
    "semver": "^6.2.0"
  },
  "optionalDependencies": {
    "@commitlint/load": ">6.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./index.js"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
